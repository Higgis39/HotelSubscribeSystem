<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.view.FillOrderFrameController">
   <children>
      <Label layoutX="335.0" layoutY="22.0" text="填写订单">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="hotelname" layoutX="311.0" layoutY="74.0" text="此处显示酒店名">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="246.0" layoutY="172.0" text="入住时间：" />
      <DatePicker fx:id="enterindate" layoutX="369.0" layoutY="168.0" onAction="#enterindateAction" prefHeight="22.0" prefWidth="135.0" />
      <Label layoutX="246.0" layoutY="217.0" text="退房时间：" />
      <DatePicker fx:id="enteroutdate" layoutX="369.0" layoutY="213.0" onAction="#enteroutdateAction" prefHeight="19.0" prefWidth="135.0" />
      <Label layoutX="246.0" layoutY="125.0" text="房间类型：" />
      <ComboBox fx:id="enterroomtype" layoutX="369.0" layoutY="122.0" prefHeight="22.0" prefWidth="135.0" />
      <Label layoutX="306.0" layoutY="389.0" text="总价：">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="price" layoutX="375.0" layoutY="391.0" prefHeight="15.0" prefWidth="51.0" text="显示价格" />
      <Label layoutX="246.0" layoutY="304.0" text="入住人数：" />
      <Label layoutX="246.0" layoutY="261.0" text="房间数量：" />
      <Separator layoutX="-1.0" layoutY="61.0" prefHeight="6.0" prefWidth="750.0" />
      <TextField fx:id="enterroomnum" alignment="CENTER" layoutX="385.0" layoutY="257.0" prefHeight="23.0" prefWidth="36.0" text="1" />
      <Button layoutX="374.0" layoutY="257.0" mnemonicParsing="false" onAction="#roomnumminusAction" text="-" />
      <Button layoutX="415.0" layoutY="257.0" mnemonicParsing="false" onAction="#roomnumplusAction" text="+" />
      <TextField fx:id="enterpeoplenum" alignment="CENTER" layoutX="385.0" layoutY="300.0" prefHeight="23.0" prefWidth="36.0" text="1" />
      <Button layoutX="415.0" layoutY="300.0" mnemonicParsing="false" onAction="#peoplenumplusAction" text="+" />
      <Button layoutX="374.0" layoutY="300.0" mnemonicParsing="false" onAction="#peoplenumminusAction" text="-" />
      <Label layoutX="246.0" layoutY="345.0" text="有无儿童：" />
      <ComboBox fx:id="enterhaschild" layoutX="373.0" layoutY="342.0" prefHeight="22.0" prefWidth="72.0" />
      <Label layoutX="306.0" layoutY="417.0" text="请您确认信息后再下单！" textFill="#f80000" />
      <Button layoutX="264.0" layoutY="450.0" mnemonicParsing="false" onAction="#confirmAction" prefHeight="19.0" prefWidth="72.0" text="下单" />
      <Button layoutX="415.0" layoutY="450.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="19.0" prefWidth="72.0" text="取消" />
      <Label fx:id="indateword" layoutX="524.0" layoutY="173.0" prefHeight="15.0" prefWidth="211.0" textFill="RED" />
      <Label fx:id="outdateword" layoutX="524.0" layoutY="217.0" prefHeight="15.0" prefWidth="211.0" textFill="RED" />
      <Label fx:id="roomtypeword" layoutX="524.0" layoutY="125.0" prefHeight="15.0" prefWidth="211.0" textFill="RED" />
      <Label fx:id="haschildword" layoutX="464.0" layoutY="345.0" prefHeight="15.0" prefWidth="220.0" textFill="#ee0000" />
      <Button layoutX="437.0" layoutY="387.0" mnemonicParsing="false" onAction="#getTotalAction" text="得到总价" />
   </children>
</AnchorPane>
