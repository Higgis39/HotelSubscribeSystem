      <Button layoutX="343.0" layoutY="253.0" mnemonicParsing="false" onAction="#roomnumplusAction" text="+" />
      <TextField fx:id="enterpeoplenum" alignment="CENTER" layoutX="309.0" layoutY="296.0" prefHeight="23.0" prefWidth="36.0" text="1" />
      <Button layoutX="343.0" layoutY="296.0" mnemonicParsing="false" onAction="#peoplenumplusAction" text="+" />
      <Button layoutX="288.0" layoutY="296.0" mnemonicParsing="false" onAction="#peoplenumminusAction" prefHeight="22.0" prefWidth="25.0" text="-" />
